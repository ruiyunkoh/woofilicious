{{#extends 'base'}}

{{#block 'content'}}
  <h1>Register an Account</h1>
  <div class="form-holder">
  <form method="POST" class="row">
      {{{form}}}
      <input type="submit" value="Sign Up" class="create-btn btn-outline-secondary col-1 my-3"/>
      <input type="hidden" name="_csrf" value="{{csrfToken}}"/>
  </form>
</div>
{{/block}}

{{/extends}}
